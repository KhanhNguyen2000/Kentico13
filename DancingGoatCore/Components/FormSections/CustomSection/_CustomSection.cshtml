@using Kentico.Forms.Web.Mvc
@using DancingGoat.Components.FormSections.CustomSection


@model FormSectionViewModel<CustomSectionProperties>

@if (!string.IsNullOrEmpty(Model?.Properties?.Title))
{
    @Html.Raw(Html.Kentico().ResolveRichText(Model.Properties.Title))
}


<div class="row custom-row">
    <div class="col-md-12 custom-section">
        @await Html.Kentico().FormZoneAsync()
    </div>
</div>
   
